tu1 -dciteseer -t0 -eRoughTune -r3 -oGraphSage;

# G2 - g7-g9
tu2 -dairport -t0 -eRoughTune -r3 -oGCN;
tu2 -dairport -t0 -eRoughTune -r3 -oGAT;
tu2 -dairport -t0 -eRoughTune -r3 -oGraphSage;

# G3 - g10-g12
