########################Aug 11 Final Finetune ##########################
########################################################################

Arxiv 3 g0,1,2,3
Cora+Citeseer+Airport g4,g5
Blog+Flickr g6, g7


<<<<<<<<  Arxiv g0, g1, g2, g3 >>>>>>>>
# g1~g3 Forward
tu0 -darxiv -t0 -eFineTuneGAT -r5;
tu0 -darxiv -t0 -eFineTuneGraphSage -r5;
tu0 -darxiv -t0 -eFineTuneGCN -r5;

# g4~g6 Backward
tu1 -darxiv -t0 -eFineTuneGCN -r5 -v;
tu1 -darxiv -t0 -eFineTuneGAT -r5 -v;
tu1 -darxiv -t0 -eFineTuneGraphSage -r5 -v;

# g7~9 Forward 1/4
tu2 -darxiv -t0 -eFineTuneGAT -r5 -s0.25;
tu2 -darxiv -t0 -eFineTuneGraphSage -r5 -s0.25;
tu2 -darxiv -t0 -eFineTuneGCN -r5 -s0.25;

# g10~g12 Backward 1/4
tu3 -darxiv -t0 -eFineTuneGCN -r5 -v -s0.25;
tu3 -darxiv -t0 -eFineTuneGAT -r5 -v -s0.25;
tu3 -darxiv -t0 -eFineTuneGraphSage -r5 -v -s0.25;

<<<<<<<<  Cora+ Citeseer: g4,g5 >>>>>>>>

# g13~g18 Cora
tu4 -dcora -t0 -eFineTuneGCN -r5;
tu4 -dcora -t0 -eFineTuneGAT -r5;
tu4 -dcora -t0 -eFineTuneGraphSage -r5;
tu5 -dcora -t0 -eFineTuneGCN -r5 -v;
tu5 -dcora -t0 -eFineTuneGAT -r5 -v;
tu5 -dcora -t0 -eFineTuneGraphSage -r5 -v;

# g19~g24 Citeseer
tu4 -dciteseer -t0 -eFineTuneGCN -r5;
tu4 -dciteseer -t0 -eFineTuneGAT -r5;
tu4 -dciteseer -t0 -eFineTuneGraphSage -r5;
tu5 -dciteseer -t0 -eFineTuneGCN -r5 -v;
tu5 -dciteseer -t0 -eFineTuneGAT -r5 -v;
tu5 -dciteseer -t0 -eFineTuneGraphSage -r5 -v;

<<<<<<<<  Blog+ Flickr: g6,g7 >>>>>>>>
# g25~g30 Blogcatalog
tu6 -dblogcatalog -t0 -eFineTuneGCN -r5;
tu6 -dblogcatalog -t0 -eFineTuneGAT -r5;
tu6 -dblogcatalog -t0 -eFineTuneGraphSage -r5;
tu7 -dblogcatalog -t0 -eFineTuneGCN -r5 -v;
tu7 -dblogcatalog -t0 -eFineTuneGAT -r5 -v;
tu7 -dblogcatalog -t0 -eFineTuneGraphSage -r5 -v;

# g31~g36 Flickr
tu6 -dflickr -t0 -eFineTuneGCN -r5;
tu6 -dflickr -t0 -eFineTuneGAT -r5;
tu6 -dflickr -t0 -eFineTuneGraphSage -r5;
tu7 -dflickr -t0 -eFineTuneGCN -r5 -v;
tu7 -dflickr -t0 -eFineTuneGAT -r5 -v;
tu7 -dflickr -t0 -eFineTuneGraphSage -r5 -v;

<<<<<<<<  Airport: Misc >>>>>>>>

# g37~g42 Airport
tu4 -dairport -t0 -eFineTuneGraphSage -r5;
tu5 -dairport -t0 -eFineTuneGCN -r5;
tu6 -dairport -t0 -eFineTuneGAT -r5;
tu7 -dairport -t0 -eFineTuneGraphSage -r5 -v;
tu4 -dairport -t0 -eFineTuneGCN -r5 -v;
tu5 -dairport -t0 -eFineTuneGAT -r5 -v;

