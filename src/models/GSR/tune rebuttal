########################Aug 08 GNN backbone##########################

# g1-4 Arxiv

tu0 -darxiv -t0 -eRoughTuneGAT -r3;
tu1 -darxiv -t0 -eRoughTuneGraphSage -r3;
tu2 -darxiv -t0 -eRoughTuneGAT -r3 -v;
tu3 -darxiv -t0 -eRoughTuneGraphSage -r3 -v;

# g5-7
tu4 -dcora -t0 -eRoughTuneGAT -r3; tu4 -dflickr -t0 -eRoughTuneGraphSage -r3 -v;
tu4 -dcora -t0 -eRoughTuneGraphSage -r3;tu4 -dblogcatalog -t0 -eRoughTuneGAT -r3 -v;
tu4 -dciteseer -t0 -eRoughTuneGAT -r3;

# g8-10
tu5 -dciteseer -t0 -eRoughTuneGraphSage -r3;tu5 -dflickr -t0 -eRoughTuneGAT -r3 -v;
tu5 -dairport -t0 -eRoughTuneGAT -r3; tu5 -dblogcatalog -t0 -eRoughTuneGraphSage -r3 -v;
tu5 -dairport -t0 -eRoughTuneGraphSage -r3;

# g11-12
tu6 -dblogcatalog -t0 -eRoughTuneGAT -r3;
tu6 -dblogcatalog -t0 -eRoughTuneGraphSage -r3;

# g13-14
tu7 -dflickr -t0 -eRoughTuneGAT -r3;
tu6 -dflickr -t0 -eRoughTuneGraphSage -r3;

tu4 -dairport -t0 -eRoughTuneGAT -r3 -v;
tu6 -dairport -t0 -eRoughTuneGraphSage -r3 -v;
tu7 -dblogcatalog -t0 -eRoughTuneGraphSage -r3 -v;


########################Aug 08 GNN backbone##########################

# g1-4 Arxiv

